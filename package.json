{
	"scripts": {
		"serve": "cross-env NODE_ENV=development concurrently \"postcss public/styles/index.pcss -o public/styles/index.css --watch\"  \"live-server ./public\"",
		"development": "cross-env NODE_ENV=development postcss public/styles/index.pcss -o public/styles/index.css",
		"production": "cross-env NODE_ENV=production postcss public/styles/index.pcss -o public/styles/index.css",
		"start": "npm run serve"
	},
	"devDependencies": {
		"autoprefixer": "^10.0.2",
		"concurrently": "^5.3.0",
		"cross-env": "^7.0.2",
		"cssnano": "^4.1.10",
		"eslint": "^7.11.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-html": "^6.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"live-server": "^1.2.1",
		"postcss": "^8.1.10",
		"postcss-cli": "^8.3.0",
		"postcss-nested": "^5.0.1",
		"precss": "^4.0.0",
		"prettier": "^2.1.2",
		"stylelint": "^13.7.2",
		"stylelint-config-idiomatic-order": "^8.1.0",
		"stylelint-order": "^4.1.0",
		"tailwindcss": "^2.0.1"
	}
}
